{
  "name": "speedtracker",
  "version": "1.2.3",
  "description": "Website performance monitoring tool",
  "main": "index.js",
  "scripts": {
    "watch:webpack": "webpack-dev-server --hot --mode development",
    "watch:jekyll": "bundle exec jekyll build -w -d _site --future --drafts",
    "start": "run-p watch:*",
    "build": "webpack --mode production",
    "lint": "standard --verbose | snazzy"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": "speedtracker/speedtracker",
  "author": "Eduardo Bou√ßas <mail@eduardoboucas.com> (https://eduardoboucas.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "npm-run-all": "^4.1.5",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "chart.js": "^2.2.2",
    "es6-promise": "^4.2.5",
    "object-path": "^0.11.2",
    "query-string": "^6.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "whatwg-fetch": "^3.0.0"
  }
}
